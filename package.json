{
  "name": "tradebinder",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "deploy": "wrangler pages deploy dist",
    "worker:dev": "wrangler dev",
    "worker:deploy": "wrangler deploy",
    "db:migrate": "wrangler d1 execute tradebinder-db --local --file=./db/schema.sql",
    "db:migrate:remote": "wrangler d1 execute tradebinder-db --remote --file=./db/schema.sql",
    "db:seed": "wrangler d1 execute tradebinder-db --local --file=./db/seed.sql"
  },
  "dependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.22.0",
    "zustand": "^4.5.0"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20240208.0",
    "@types/react": "^18.2.55",
    "@types/react-dom": "^18.2.19",
    "@vitejs/plugin-react": "^4.2.1",
    "vite": "^5.1.0",
    "wrangler": "^3.28.0",
    "typescript": "^5.3.3"
  }
}